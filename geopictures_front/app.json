{
  "expo": {
    "name": "geopictures_front",
    "slug": "geopictures_front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.geopictures.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.549351708019-ggoc5m3p20e8begl25tl1a87v7c1v3uo"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.geopictures.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
